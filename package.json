{
  "name": "wsls",
  "version": "0.0.2",
  "description": "Special 'ls' to list projects in your workspace.",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ksoichiro/wsls.git"
  },
  "bugs": {
    "url": "https://github.com/ksoichiro/wsls/issues"
  },
  "main": "lib/index.js",
  "bin": "lib/cli.js",
  "author": {
    "name": "Soichiro Kashima",
    "email": "soichiro.kashima@gmail.com",
    "url": "https://github.com/ksoichiro"
  },
  "engines": {
    "node": ">=0.12.0"
  },
  "keywords": [
    "cli"
  ],
  "dependencies": {
    "async": "^1.5.2",
    "chalk": "^1.1.3",
    "git-config": "0.0.7",
    "ini": "^1.3.4",
    "moment": "^2.12.0",
    "svn-spawn": "^0.1.5"
  },
  "devDependencies": {
    "ava": "^0.13.0",
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "eslint": "^2.5.3",
    "temp": "^0.8.3"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "files": [
    "src",
    "lib"
  ],
  "scripts": {
    "prepublish": "npm run build",
    "build": "babel src -d lib -s inline",
    "watch": "babel src -d lib -s inline -w",
    "test": "eslint . && ava"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "sourceType": "module"
    },
    "rules": {
      "indent": [
        "error",
        2
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "always"
      ],
      "no-console": [
        "off"
      ]
    }
  }
}
